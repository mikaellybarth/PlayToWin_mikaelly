<h1>Usuarios cadastrados</h1>

<table border="1">
    <tr>  <!-- table row -->
        <th>Id</th> <!-- table head -->
        <th>Nickname</th>
        <th>Nome</th>
    </tr>
    {{#each usuarios}}
      <tr>
       <td>{{ this.id }}</td>
       <td>{{ this.nickname }}</td>
       <td>{{ this.nome }}</td>
      </tr>
    {{/each}}
</table>